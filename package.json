{
  "name": "better-el-ref",
  "version": "1.0.7",
  "description": "A better way to reference HTML elements",
  "main": "lib/index.js",
  "author": "Drew Markel",
  "homepage": "https://github.com/drewmrk/better-el-ref",
  "repository": "https://github.com/drewmrk/better-el-ref",
  "license": "MIT",
  "keywords": [
    "front-end",
    "html",
    "typescript",
    "ref",
    "element",
    "document"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "eslint-config-google": "0.14.0",
    "jest": "27.0.1",
    "prettier": "2.3.0",
    "ts-jest": "27.0.1",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "scripts": {
    "build": "yarn clean && jest && tsc && prettier --write \"lib/*.js\" \"lib/*.ts\"",
    "format": "prettier --write \"src/*.ts\" \"tests/*.ts\"",
    "clean": "rm -rf lib",
    "test": "jest"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md"
  ]
}
