{
  "name": "better-el-ref",
  "version": "2.0.5",
  "description": "A better way to reference HTML elements",
  "main": "lib/index.js",
  "author": "Drew Markel",
  "homepage": "https://github.com/drewmrk/better-el-ref",
  "repository": "https://github.com/drewmrk/better-el-ref",
  "license": "MIT",
  "keywords": [
    "front-end",
    "html",
    "typescript",
    "ref",
    "element",
    "document"
  ],
  "devDependencies": {
    "@babel/preset-env": "7.14.4",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "eslint": "7.27.0",
    "eslint-config-google": "0.14.0",
    "jest": "27.0.3",
    "prettier": "2.3.0",
    "ts-jest": "27.0.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "scripts": {
    "build": "yarn clean && tsc && prettier --write \"lib/**/*.js\" \"lib/**/*.ts\" && yarn test",
    "format": "prettier --write \"src/**/*.ts\" \"spec/**/*.ts\"",
    "clean": "rm -rf lib",
    "test": "jest"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md"
  ]
